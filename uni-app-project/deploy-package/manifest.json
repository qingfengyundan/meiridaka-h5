{
    "name": "每日打卡",
    "appid": "__UNI__MEIRIDAKA",
    "description": "培养自律好习惯的每日打卡应用",
    "versionName": "1.0.0",
    "versionCode": "100",
    "transformPx": false,
    "app-plus": {
        "usingComponents": true,
        "nvueStyleCompiler": "uni-app",
        "compilerVersion": 3,
        "splashscreen": {
            "alwaysShowBeforeRender": true,
